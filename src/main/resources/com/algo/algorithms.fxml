<VBox alignment="CENTER" prefHeight="480.0" prefWidth="640.0" spacing="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.algo.AlgorithmsController">
    <Text text="Algorithms" style="-fx-font-size: 24px;"/>
    <Button text="Bubble Sort" onAction="#handleBubbleSort"/>
    <Button text="Selection Sort" onAction="#handleSelectionSort"/>
    <Button text="Insertion Sort" onAction="#handleInsertionSort"/>
    <Button text="Binary Search" onAction="#handleBinarySearch"/>
    <Button text="Back" onAction="#handleBack"/>
</VBox>